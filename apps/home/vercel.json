{
  "rewrites": [
    {
      "source": "/mac/:path*",
      "destination": "https://multi-zones-nextjs-mac.vercel.app/mac/:path*"
    },
    {
      "source": "/store/:path*",
      "destination": "https://multi-zones-nextjs-store.vercel.app/store/:path*"
    },
    {
      "source": "/profile/:path*",
      "destination": "https://multi-zones-nextjs-profile.vercel.app/profile/:path*"
    },
    {
      "source": "/card/:path*",
      "destination": "https://multi-zones-nextjs-card.vercel.app/card/:path*"
    }
  ]
}